@model Nop.Plugin.Seurata.GoogleShopping.Models.ExpressModels.SeurataExpressOrderItemModel


<div class="panel-body-order">
    <div class="row">
        <div class="col-lg-5 col-md-5 col-sm-4 col-xs-12">
            <div class="form-group">
                <div class="col-md-6">
                    <div class="label-wrapper"><label class="control-label">Brand Name </label></div>
                </div>
                <div class="col-md-6">
                    @Model.BrandName
                </div>
            </div>

            <div class="form-group">
                <div class="col-md-6">
                    <div class="label-wrapper"><label class="control-label">Shipping Carrier</label></div>
                </div>
                <div class="col-md-6">
                    @Model.ShippingCarrier
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-6">
                    <div class="label-wrapper"><label class="control-label">Min Days In Transit</label></div>
                </div>
                <div class="col-md-6">
                    @Model.MinDaysInTransit
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-6">
                    <div class="label-wrapper"><label class="control-label">Return Policy</label></div>
                </div>
                <div class="col-md-6">
                    <a href="@Model.ReturnPolicyUrl" target="_blank">@Model.ReturnPolicyUrl</a>
                </div>
            </div>
        </div>
        <div class="col-lg-7 col-md-7 col-sm-4 col-xs-12">
            <div class="form-group">
                <div class="col-md-4">
                    <div class="label-wrapper"><label class="control-label">Max Days In Transit</label></div>
                </div>
                <div class="col-md-6">
                    @Model.MaxDaysInTransit
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="label-wrapper"><label class="control-label">Deliver By Date</label></div>
                </div>
                <div class="col-md-6">
                    @Model.DeliverByDate
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-4">
                    <div class="label-wrapper"><label class="control-label">Ship By Date</label></div>
                </div>
                <div class="col-md-6">
                    @Model.ShipByDate
                </div>
            </div>

        </div>

    </div>



</div>
