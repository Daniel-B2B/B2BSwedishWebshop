@model Nop.Plugin.Seurata.GoogleShopping.Models.ExpressModels.SeurataExpressOrderModel
@using Nop.Web.Framework;
@using Nop.Web.Framework.UI;
@using Nop.Plugin.Seurata.GoogleShopping.Constants;
@{
    Layout = ViewNames.AdminLayout;
    Html.AddCssFileParts(ViewNames.GoogleShoppingStyles);
    Html.SetActiveMenuItemSystemName("Seurata-GoogleShopping-Express-Menu");
    ViewBag.Title = "Express Order";
}

<div class="content-header clearfix">
    <h1 class="pull-left">
        Order Details "@Model.OrderId"
        <small><i class="fa fa-arrow-circle-left"></i>@Html.ActionLink(T("Admin.Orders.BackToList").Text, "OrderList")</small>
    </h1>
</div>

<div class="content">
    <div id="product-edit">
        <div class="row">
            <div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
                <div class="OrderInfo">
                    <div>
                        @Html.RenderBootstrapTabContent("tab-info", @TabInfo(), true)
                    </div>

                </div>
                <div class="ProductInfoPanel">
                    @foreach (var item in Model.OrderItems)
                    {
                        <div class="ListPanel">
                            <div class="row marB">
                                <div class="col-lg-4 col-md-4 col-sm-4 col-lg-offset-2 col-md-offset-2  col-xs-12 hidden-xs">
                                    <div class="title">
                                        <h5>Item</h5>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-3 col-xs-12 hidden-xs">
                                    <div class="title">
                                        <h5>Quantity</h5>
                                    </div>
                                </div>

                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 hidden-xs text-right">
                                    <div class="title">
                                        <h5>Tax Amount</h5>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12 hidden-xs text-right">
                                    <div class="title">
                                        <h5>Price</h5>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-xs-12 hidden-lg hidden-md hidden-sm">
                                    <div class="title">
                                        <h5>Item</h5>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-4">
                                    <div class="ProductImage">
                                        <img src=@item.ShownImage class="img-responsive" />
                                    </div>
                                </div>


                                <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12 text-left">
                                    <div class="ProductInfo">
                                        <div class="productTitle">
                                            <h4>@*<a href="">*@ @item.ProductTitle@*</a>*@</h4>
                                        </div>
                                        <div class="ProductID">
                                            <div class="row">
                                                <div class="col-md-4 col-xs-6">
                                                    <label class="control-label"> Item Id:</label>
                                                </div>
                                                <div class="col-md-5 col-xs-6">
                                                    @item.OrderItemId
                                                </div>
                                            </div>
                                        </div>
                                        <div class="Returnable">
                                            <div class="row">
                                                <div class="col-md-4 col-xs-6">
                                                    <label class="control-label">  Returnable:</label>
                                                </div>
                                                <div class="col-md-5 col-xs-6">
                                                    <div class="">@(item.Returnable ? "Yes" : "No")</div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="Returnable">
                                            <div class="row">
                                                <div class="col-md-4 col-xs-6">
                                                    <label class="control-label">   Shipping  Method:</label>
                                                </div>
                                                <div class="col-md-5 col-xs-6">
                                                    @item.ShippingMethodName
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 hidden-lg hidden-md hidden-sm">
                                    <div class="title">
                                        <h5>Quantity</h5>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-12">
                                    <div class="ProductQuantity">
                                        @if (item.QuantityOrdered > 0)
                                        {
                                            <div class="QuantityOrder">
                                                <div class="row">
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <label class="control-label">  Ordered: </label>
                                                    </div>
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <span class="quantityList">@item.QuantityOrdered</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                        @if (item.QuantityPending > 0)
                                        {
                                            <div class="QuantityPending">
                                                <div class="row">
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <label class="control-label">   Pending: </label>
                                                    </div>
                                                    <div class="col-md-5 col-sm-5 col-xs-6"> <span class="quantityList">@item.QuantityPending</span></div>

                                                </div>
                                            </div>
                                        }
                                        @if (item.QuantityCanceled > 0)
                                        {
                                            <div class="QuantityCanceled">
                                                <div class="row">
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <label class="control-label">   Canceled:</label>
                                                    </div>
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <span class="quantityList">@item.QuantityCanceled</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                        @if (item.QuantityDelivered > 0)
                                        {
                                            <div class="QuantityDelivered">
                                                <div class="row">
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <label class="control-label">  Delivered:</label>
                                                    </div>
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <span class="quantityList">@item.QuantityDelivered</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                        @if (item.QuantityReturned > 0)
                                        {
                                            <div class="QuantityReturned">
                                                <div class="row">
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <label class="control-label">   Returned:</label>
                                                    </div>
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <span class="quantityList">@item.QuantityReturned</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                        @if (item.QuantityShipped > 0)
                                        {
                                            <div class="QuantityShipped">
                                                <div class="row">
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <label class="control-label">   Shipped:</label>
                                                    </div>
                                                    <div class="col-md-5 col-sm-5 col-xs-6">
                                                        <span class="quantityList">@item.QuantityShipped</span>
                                                    </div>
                                                </div>
                                            </div>
                                        }
                                    </div>
                                </div>

                                <div class="col-xs-6 hidden-lg hidden-md hidden-sm">
                                    <div class="title">
                                        <h5>Tax Amount</h5>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-6 text-right">
                                    <div class="ProductTax">
                                        @item.TaxAmount
                                    </div>
                                </div>

                                <div class="col-xs-6 hidden-lg hidden-md hidden-sm">
                                    <div class="title">
                                        <h5>Price</h5>
                                    </div>
                                </div>
                                <div class="col-lg-2 col-md-2 col-sm-2 col-xs-6 text-right">
                                    <div class="ProductPrice">
                                        @item.Price @*@(!string.IsNullOrEmpty(Model.CurrencyCode) ? Model.CurrencyCode : "")*@
                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="OrderItems">
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-6 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Brand Name: </label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    @item.BrandName
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-6 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Shipping Carrier:</label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    @item.ShippingCarrier
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-6 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Min Days In Transit:</label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    @item.MinDaysInTransit
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-6 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Return Policy:</label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    <a href="@item.ReturnPolicyUrl" target="_blank">@item.ReturnPolicyUrl</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-5 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Max Days In Transit:</label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    @item.MaxDaysInTransit
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-5 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Deliver By Date:</label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    @item.DeliverByDate
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="row">
                                                <div class="col-md-5 col-xs-6">
                                                    <div class="label-wrapper"><label class="control-label">Ship By Date:</label></div>
                                                </div>
                                                <div class="col-md-6 col-xs-6">
                                                    @item.ShipByDate
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                    }
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                <div class="customer">
                    <div class="ContactInfo">
                        @if (!string.IsNullOrEmpty(Model.CustomerFullName))
                        {
                            <div class="form-group">
                                <div class="row">
                                    <div class="col-md-11">
                                        <h3> @Html.NopDisplayFor(model => model.CustomerFullName)</h3>
                                    </div>
                                </div>
                            </div>
                        }
                    </div>
                    <div class="ShippingInfo">
                        <h4>@T("Plugins.Seurata.GoogleShopping.Admin.Order.Detail.ShipAddress")</h4>
                        <div>
                            @Html.RenderBootstrapTabContent("tab-ship-address", @TabShippingAddress())
                        </div>
                    </div>
                    <div class="BillingInfo">
                        <h4>@T("Plugins.Seurata.GoogleShopping.Admin.Order.Detail.BillAddress")</h4>
                        <div>
                            @Html.RenderBootstrapTabContent("tab-bill-address", @TabBillingAddress())
                        </div>
                    </div>
                    <div class="BillingInfo">
                        <h4>@T("Plugins.Seurata.GoogleShopping.Admin.Order.Detail.Shipments")</h4>
                        <div>
                            @Html.RenderBootstrapTabContent("tab-shipments", @TabShipments())
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@helper TabInfo()
{
    <div class="panel-group form-horizontal order-det">
        <div class="panel-default">
            <div class="panel-body-status">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12">

                        <div class="OrderStatus">
                            <div class="form-group">
                                <div class="col-md-5 col-xs-6">
                                    @Html.NopLabelFor(model => model.OrderStatus)
                                </div>
                                <div class="col-md-6 col-xs-6">
                                    @Html.NopDisplayFor(model => model.OrderStatus)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-5 col-xs-6">
                                    @Html.NopLabelFor(model => model.OrderPlacedDate)
                                </div>
                                <div class="col-md-6 col-xs-6">
                                    @Html.NopDisplayFor(model => model.OrderPlacedDate)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-5 col-xs-6">
                                    @Html.NopLabelFor(model => model.OrderNetAmount)
                                </div>
                                <div class="col-md-6 col-xs-6">
                                    @Model.OrderNetAmount @*@(!string.IsNullOrEmpty(Model.Currency) ? Model.Currency : "")*@
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-5 col-xs-6">
                                    @Html.NopLabelFor(model => model.ShippingOption)
                                </div>
                                <div class="col-md-6 col-xs-6">
                                    @Html.NopDisplayFor(model => model.ShippingOption)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-5 col-xs-6">
                                    @Html.NopLabelFor(model => model.PaymentStatus)
                                </div>
                                <div class="col-md-6 col-xs-6">
                                    @Html.NopDisplayFor(model => model.PaymentStatus)
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@helper TabShippingAddress()
{
    <div class="panel-group form-horizontal order-det">
        <div class="panel-default">
            <div class="panel-body-shipping">
                <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12">
                        @if (!string.IsNullOrEmpty(Model.CustomerFullName) || !string.IsNullOrEmpty(Model.ShippingFullAddress))
                        {
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingName)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingFullAddress)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingLocality)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingRegion)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingPinCode)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingCountry)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.ShippingPhone)
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@helper TabBillingAddress()
{
    <div class="panel-group form-horizontal order-det">
        <div class="panel-default">
            <div class="panel-body-billing">
                <div class="row">
                    <div class="col-xs-12 col-sm-6 col-md-6">
                        @if (!string.IsNullOrEmpty(Model.CustomerFullName) || !string.IsNullOrEmpty(Model.ShippingFullAddress))
                        {
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.BillingName)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.BillingFullAddress)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.BillingLocality)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.BillingRegion)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.BillingPinCode)
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    @Html.NopDisplayFor(model => model.BillingCountry)
                                </div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@helper TabShipments()
{
    <div class="panel-group form-horizontal order-det">
        <div class="panel-default">
            <div class="panel-body-billing">
                <div class="row">
                    @foreach (var item in Model.OrderShipments)
                    {
                        <div class="shipmentActivity">
                            <div class="col-xs-12 col-sm-12 col-md-12">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        <label class="control-label"> Shipment Id: </label>
                                    </div>
                                    <div class="col-md-5">
                                        @item.ShipmentId
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-4">
                                        <label class="control-label"> Carrier: </label>
                                    </div>
                                    <div class="col-md-5">
                                        @item.Carrier
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-4">
                                        <label class="control-label"> Tracking Id: </label>
                                    </div>
                                    <div class="col-md-5">
                                        @item.TrackingId
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-4">
                                        <label class="control-label"> Status: </label>
                                    </div>
                                    <div class="col-md-5">
                                        @item.Status
                                    </div>
                                </div>
                                @if (item.DeliveryDate != null)
                                {
                                    <div class="form-group">
                                        <div class="col-md-4">
                                            <label class="control-label"> Delivery Date: </label>
                                        </div>
                                        <div class="col-md-5">
                                            @item.DeliveryDate
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    }
                </div>
            </div>
        </div>
    </div>
}

<div class="panel-body">
    <div class="text-center"><a href="https://www.seurata.com/" target="_blank"><img alt="https://www.seurata.com" src="data:image/png;base64,"></a></div>
</div>
