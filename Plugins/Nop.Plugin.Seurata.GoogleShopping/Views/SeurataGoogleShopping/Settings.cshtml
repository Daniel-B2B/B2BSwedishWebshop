    var clickedCustomLabelName = "";
    $(document).ready(function () {
        if ($('#MerchantId').val() !== '') {
            $('#gconfig-btn').show();
        }

        $("#@Html.FieldIdFor(model => model.SetCustomLabels)").click(toggleCustomLabelEnabled);
        toggleCustomLabelEnabled();

        CheckGAConfigured();
        CheckForUpdates();

        CheckExpressTablesInstalled();

        $('#search-category-name').autocomplete({
            delay: 500,
            minLength: 3,
            scroll: true,
            source: "@Url.Action("GoogleCategorySearchAutoComplete", "SeurataGoogleShopping")",
            select: function (event, ui) {
                $('#DefaultGoogleCategory').val(ui.item.labelValue);
                $('#search-category-friendly-name').text(ui.item.label);
                $('#search-category-clear').show();
                $('#search-category-name').val('');
                return false;
            }
        });

        $('#search-category-clear').click(function () {
            $('#DefaultGoogleCategory').val('');
            $('#search-category-friendly-name').text('');
            $('#search-category-clear').hide();
            return false;
        });

        $("#stop-running-task-settings").on('click', function () {
            $.ajax({
                cache: false,
                type: "POST",
                url: "@(Url.Action("StopRunningTask", "SeurataGoogleShopping"))",
                success: function (data) {

                },
                error: function (xhr, ajaxOptions, thrownError) {
                },
                traditional: true
            });
        });

        $('#MerchantId').bind('input propertychange', function () {
            if ($('#MerchantId').val().length > 3) {
                $('#gconfig-btn').show();
                $('#merchantId_msg').show();
            }
            else {
                $('#gconfig-btn').hide();
                $('#merchantId_msg').hide();
            }
        });

        if ('@TempData["SavedSettings"]' !== null && '@TempData["SavedSettings"]' === 'True') {
            $('#sync_locally').trigger('click');
        }

        DisableGlobalCustomLabels();
    });

    function DisableGlobalCustomLabels() {
        @if(!string.IsNullOrEmpty(Model.CustomLabel0DisplayValues))
        {
            @:$("#CustomLabel0").attr("disabled", "disabled");
        }
        else
        {
             @:$("#CustomLabel0").removeAttr("disabled");
        }

        @if(!string.IsNullOrEmpty(Model.CustomLabel1DisplayValues))
        {
            @:$("#CustomLabel1").attr("disabled", "disabled");
        }
        else
        {
             @:$("#CustomLabel1").removeAttr("disabled");
        }

        @if(!string.IsNullOrEmpty(Model.CustomLabel2DisplayValues))
        {
            @:$("#CustomLabel2").attr("disabled", "disabled");
        }
        else
        {
             @:$("#CustomLabel2").removeAttr("disabled");
        }

        @if(!string.IsNullOrEmpty(Model.CustomLabel3DisplayValues))
        {
            @:$("#CustomLabel3").attr("disabled", "disabled");
        }
        else
        {
             @:$("#CustomLabel3").removeAttr("disabled");
        }

        @if(!string.IsNullOrEmpty(Model.CustomLabel4DisplayValues))
        {
            @:$("#CustomLabel4").attr("disabled", "disabled");
        }
        else
        {
             @:$("#CustomLabel4").removeAttr("disabled");
        }

        @if(!string.IsNullOrEmpty(Model.ShippingLabelDisplayValues))
        {
            @:$("#ShippingLabel").attr("disabled", "disabled");
        }
        else
        {
             @:$("#ShippingLabel").removeAttr("disabled");
        }
    }

    function GetAddLogicPartialView(handle, labelValue) {
        clickedCustomLabelValue = labelValue;
        var labelName = $(handle).data("name");
        clickedCustomLabelName = labelName;

        $.ajax({
            cache: false,
            type: "POST",
            url: "@(Url.Action("GetAddLogicPartialView", "SeurataGoogleShopping"))",
            success: function (data) {
                if (data !== '') {
                    $("#logicmodalbodyData").html(data.PartialViewData);
                    $('#logicModal').modal({
                        backdrop: 'static',
                        keyboard: false
                    });
                    $('.modal-title').text(labelName + ' value(s)');
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
    }

    function toggleCustomLabelEnabled() {
        if ($('#@Html.FieldIdFor(model => model.SetCustomLabels)').is(':checked')) {
            $('#pnlcustomlbl').show();
        } else {
            $('#pnlcustomlbl').hide();
        }
    }

    function PopulateLanguageDropdown(country, override) {
        $("#Language").empty();
        $("#Language").append(
            $('<option/>', {
                value: 0,
                text: "Please select Target Country"
            }));
        $.ajax({
            type: "POST",
            data: JSON.stringify({ countryId: country, overrideStore: override }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            url: "@(Url.Action("GetLanguagesByCountry", "SeurataGoogleShopping"))",
            success: function (data) {
                if (data.Languages !== '') {
                    $("#Language").empty();
                    var array = data.Languages.split(",");
                    $.each(array, function (index, item) {
                        var lang = jQuery.trim(item).split(":");
                        $("#Language").append(
                            $('<option/>', {
                                value: lang,
                                text: lang[0] + " (" + lang[1] + ")"
                            }));
                    });
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
    }

    function PopulateCurrencyField(country, override) {
        $("#currency-div").empty().html('Please select Target Country');
        $.ajax({
            type: "POST",
            data: JSON.stringify({ countryId: country, overrideStore: override }),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            url: "@(Url.Action("GetCurrencyByCountry", "SeurataGoogleShopping"))",
            success: function (data) {
                if (data.Currency !== '') {
                    $("#Currency").val(data.Currency);
                    $("#currency-div").empty().html(data.Currency);
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
    }

    function CheckSyncService() {
        $("#ajaxBusy").hide();
        $.ajax({
            cache: false,
            type: "GET",
            url: "@(Url.Action("CheckFeedRunning", "SeurataGoogleShopping"))",
            success: function (data) {
                if (data.Success) {
                    $("#syncing-div").show();
                    $("#spinner-sync").show();
                    $("#sync-message").empty().html(data.Message);
                    $("#spinner-sync-success").hide();
                    $("#spinner-sync-error").hide();
                    $("#stop-running-task-settings").show();
                    if (!data.Complete && data.Message !== '' && data.ProdListUrl !== '') {
                        location.replace(data.ProdListUrl);
                    }
                } else if (data.Message !== '') {
                    ClearInterval();
                    $("#syncing-div").show();
                    $("#spinner-sync").hide();
                    $("#sync-message").empty().html(data.Message);
                    if (data.Complete) {
                        $("#spinner-sync-error").hide();
                        $("#spinner-sync-success").show();
                    } else {
                        $("#spinner-sync-success").hide();
                        $("#spinner-sync-error").show();
                    }
                    $("#stop-running-task-settings").hide();
                    StartIntervalOnListToHideNotif(15);
                } else {
                    ClearInterval();
                    $("#syncing-div").hide();
                    $("#spinner-sync").hide();
                    $("#sync-message").empty();
                    $("#spinner-sync-success").hide();
                    $("#spinner-sync-error").hide();
                    $("#stop-running-task-settings").hide();
                    StartIntervalOnListToHideNotif(15);
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
    }

    function CheckGoogleAccountIntegrated() {
        var isReIntegrate = '@Model.ReAuth';
        $("#ajaxBusy").hide();
        $.ajax({
            type: "POST",
            data: JSON.stringify({ merchantId: $('#MerchantId').val(), reIntegrate: isReIntegrate }),
            url: "@(Url.Action("CheckGAccountConfigured", "SeurataGoogleShopping"))",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data.Success) {
                    $("#syncing-div").show();
                    $("#spinner-sync").hide();
                    $('#module-2').show();
                    $("#sync-message").empty().html("Google Account successfully intergrated, Select Target Country with other options and click on Save.");
                    $("#spinner-sync-success").show();
                    $("#spinner-sync-error").hide();
                    $("#gbtn-span-text").html('@T("Plugins.Seurata.GoogleShopping.ReIntegrateGoogle.Title")');
                    if (authwinref !== '') {
                        authwinref.close();
                    }
                    if (interval_gconfig !== '') {
                        clearInterval(interval_gconfig);
                        interval_gconfig = '';
                    }
                    interval_gconfig = setInterval(HideNotificationOnGAConfig, (20 * 1000));
                } else {
                    $("#gbtn-span-text").html('@T("Plugins.Seurata.GoogleShopping.IntegrateGoogle.Title")');
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
        $("#ajaxBusy").hide();
    }

    function CheckGAConfigured() {
        $("#ajaxBusy").hide();
        $.ajax({
            type: "POST",
            data: JSON.stringify({ merchantId: $('#MerchantId').val(), reIntegrate: false }),
            url: "@(Url.Action("CheckGAccountConfigured", "SeurataGoogleShopping"))",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                if (data.Success) {
                    $('#module-2').show();
                }
            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
        $("#ajaxBusy").hide();
    }

    function StartLocalSync() {
        $.ajax({
            cache: false,
            type: "POST",
            url: "@(Url.Action("GenerateFeeds", "SeurataGoogleShopping"))",
            success: function (data) {

            },
            error: function (xhr, ajaxOptions, thrownError) {
            },
            traditional: true
        });
        StartInterval(5);
    }

    function CheckForUpdates() {
		$("#updateRequest").empty().html("Checking for updates...");
        $.ajax({
            url: "@(Url.Action("CheckForUpdates", "SeurataGoogleShopping"))",
            type: 'POST',
            success: function (data) {
                if (data.Success) {
                    if (data.UpdateAvailable) {
                        $("#updateRequest").html("New Plugin version <b>" + data.NewVersion + "</b> available. Please download from <a href='" + data.PurchaseLink + "' target='_blank'>here</a>");
					}
					else {
						$("#updateRequest").empty().html("You have the latest version installed.");
					}
				}
				else {
					$("#updateRequest").empty().html("You have the latest version installed.");
				}
            },
            error: function (xhr, textStatus, errorThrown) {
            }
        });
    }

    function CheckExpressTablesInstalled() {
        $.ajax({
            url: "@(Url.Action("CheckAndInsertTable", "SeurataGoogleShopping"))",
            type: 'GET',
            success: function (data) {
            },
            error: function (xhr, textStatus, errorThrown) {
            }
        });
    }
</script>
