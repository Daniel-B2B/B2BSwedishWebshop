@model System.Collections.Generic.List<KeyValuePair<string, string>>

@foreach (var data in Model)
{
    <div class="bs-example">
        @if (data.Value.Equals("error", StringComparison.InvariantCultureIgnoreCase))
        {
            <div class="alert alert-danger">@data.Key</div>
        }
        else if (data.Value.Equals("critical", StringComparison.InvariantCultureIgnoreCase))
        {
            <div class="alert alert-warning">@data.Key</div>
        }
        else
        {
            <div class="alert alert-info">@data.Key</div>
        }
    </div>
}

@if (Model.Count == 0)
{
    <div class="bs-example">
        <div class="alert alert-info">Please open feed to view variant wise error(s).</div>
    </div>
}
